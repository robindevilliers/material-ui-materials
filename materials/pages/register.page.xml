<?schema version="2"?>
<page defnVersion="v1" id="register" title="Register" group="public">
    <div v="v1" orientation="COLUMN" justifyContent="SPACE_AROUND" alignItems="CENTER">
        <div v="v1" orientation="ROW" justifyContent="SPACE_AROUND" alignItems="CENTER" padding="1em">
            <image v="v1" id="title-img" src="logo.svg" width="200px" height="200px"
                   margin="0 5em 2em 0"/>
            <div v="v1" orientation="COLUMN" justifyContent="SPACE_AROUND" alignItems="CENTER">
                <title size="LARGE" v="v1">
                    <textual>Registration</textual>
                </title>
                <text v="v1">
                    <textual>
                        Welcome to Example Materials. Please fill in the form below and our staff will be in touch
                        to finalise your registration.
                    </textual>
                </text>
            </div>
        </div>
        <register-panel v="v1"/>
    </div>
</page>
